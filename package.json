{
  "name": "arab-telegram-bot",
  "version": "1.0.0",
  "description": "Advanced Arabic Telegram Bot with games, economy, content system, and group protection",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"test/**/*.js\""
  },
  "keywords": [
    "telegram",
    "bot",
    "arabic",
    "games",
    "economy"
  ],
  "author": "TareqFX",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^1.42.0",
    "axios": "^1.13.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "https-proxy-agent": "^7.0.6",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.45",
    "mongodb": "^7.1.0",
    "mongoose": "^7.5.0",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "p-retry": "^7.1.1",
    "socks-proxy-agent": "^8.0.5",
    "telegraf": "^4.14.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "eslint": "^8.57.0",
    "jest": "^25.0.0",
    "nodemon": "^1.0.2",
    "prettier": "^3.1.1"
  }
}
